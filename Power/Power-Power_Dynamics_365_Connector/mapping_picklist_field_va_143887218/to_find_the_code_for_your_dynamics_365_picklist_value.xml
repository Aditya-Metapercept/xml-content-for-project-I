         <othermeta name="title"
                    content="To find the code for your Dynamics 365 picklist value"/>
         <othermeta name="shortdesc"
                    content="Retrieve the numeric code for a picklist value in Dynamics 365 so it can be mapped correctly to a matching value in Power."/>
         <othermeta name="language" content="en-GB"/>
         <othermeta name="version" content="1.0.0"/>
         <othermeta name="docStage" content="approved"/>
