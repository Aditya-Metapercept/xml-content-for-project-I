   <title>Transfer from Maintenance Bypass Operation to Normal Operation</title>
   <shortdesc>
      <keyword>Reality 25–100 kVA NAM</keyword>
   </shortdesc>
   <prolog>
      <author>Pallavi</author>
      <critdates>
         <created date="2025-11-18+05:30"/>
         <revised modified="2025-11-18"/>
      </critdates>
