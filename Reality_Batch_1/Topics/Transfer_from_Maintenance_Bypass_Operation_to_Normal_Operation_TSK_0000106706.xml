            <prodname>metR-PXC-Reality</prodname>
            <platform>web</platform>
         </prodinfo>
         <othermeta name="title"
                    content="Transfer_from_Maintenance_Bypass_Operation_to_Normal_Operation_TSK_0000106706"/>
