            <indexterm>disable-eco-mode-check</indexterm>
            <indexterm>confirm-bypass-transfer</indexterm>
            <indexterm>verify-load-transfer</indexterm>
            <indexterm>monitor-bypass-alarms</indexterm>
            <!-- Context layer -->
            <indexterm>ups-operation</indexterm>
            <indexterm>ups-troubleshooting</indexterm>
            <indexterm>maintenance-window</indexterm>
            <indexterm>critical-power-supply</indexterm>
