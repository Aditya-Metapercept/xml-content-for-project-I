            <indexterm>signal-cable</indexterm>
            <indexterm>ups-cabinet-interface</indexterm>
            <indexterm>monitoring-board</indexterm>
            <indexterm>bbb-connection</indexterm>
            <indexterm>communication-harness</indexterm>
            <!-- Process layer -->
